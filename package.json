{
  "name": "@elindorath/eslint-plugin",
  "description": "An Eslint plugin centralizing all the configurations I use",
  "version": "1.0.0",
  "license": "MIT",
  "repository": {
    "type": "git",
    "url": "github:elindorath/eslint-plugin"
  },
  "engines": {
    "node": "24.10.0"
  },
  "packageManager": "yarn@4.10.3",
  "main": "src/index.js",
  "files": [
    "src/**"
  ],
  "type": "module",
  "scripts": {
    "extract-meta": "node scripts/extract-meta.ts",
    "lint:fix": "NODE_OPTIONS='--experimental-require-module' eslint --concurrency auto --fix .",
    "lint": "NODE_OPTIONS='--experimental-require-module' eslint --concurrency auto .",
    "test-update": "./scripts/test-update.ts",
    "type-check": "tsc",
    "update": "node scripts/generate-foreign && node scripts/generate-index && node scripts/generate-config.js"
  },
  "dependencies": {
    "@eslint-community/eslint-plugin-eslint-comments": "4.5.0",
    "@eslint/compat": "1.4.0",
    "@eslint/json": "0.13.2",
    "@eslint/markdown": "7.2.0",
    "@stylistic/eslint-plugin": "5.3.1",
    "eslint": "9.36.0",
    "eslint-import-resolver-typescript": "4.4.4",
    "eslint-plugin-array-func": "5.1.0",
    "eslint-plugin-canonical": "5.1.3",
    "eslint-plugin-eslint-plugin": "7.0.0",
    "eslint-plugin-filenames-simple": "0.9.0",
    "eslint-plugin-import-x": "4.16.1",
    "eslint-plugin-jest": "29.0.1",
    "eslint-plugin-jest-dom": "5.5.0",
    "eslint-plugin-jest-extended": "3.0.1",
    "eslint-plugin-jest-formatting": "3.1.0",
    "eslint-plugin-json-files": "5.1.2",
    "eslint-plugin-jsonc": "2.21.0",
    "eslint-plugin-jsx-a11y": "6.10.2",
    "eslint-plugin-listeners": "1.5.1",
    "eslint-plugin-n": "17.21.3",
    "eslint-plugin-no-constructor-bind": "2.0.4",
    "eslint-plugin-no-secrets": "2.2.1",
    "eslint-plugin-no-unsanitized": "4.1.4",
    "eslint-plugin-no-use-extend-native": "0.7.2",
    "eslint-plugin-perfectionist": "4.15.1",
    "eslint-plugin-promise": "7.2.1",
    "eslint-plugin-react": "7.37.5",
    "eslint-plugin-react-hooks": "5.2.0",
    "eslint-plugin-react-i18n": "1.0.3",
    "eslint-plugin-react-native": "5.0.0",
    "eslint-plugin-react-native-a11y": "3.5.1",
    "eslint-plugin-react-perf": "3.3.3",
    "eslint-plugin-security": "3.0.1",
    "eslint-plugin-sonarjs": "3.0.5",
    "eslint-plugin-ternary": "2.0.0",
    "eslint-plugin-unicorn": "61.0.2",
    "fs-extra": "11.3.2",
    "globals": "16.4.0",
    "is-what": "5.5.0",
    "jsonc-eslint-parser": "2.4.1",
    "read-package-up": "11.0.0",
    "typescript": "5.9.3",
    "typescript-eslint": "8.45.0"
  },
  "devDependencies": {
    "@sidvind/better-ajv-errors": "3.0.1",
    "@types/eslint-plugin-jsx-a11y": "6.10.1",
    "@types/eslint-plugin-security": "3.0.0",
    "@types/fs-extra": "11.0.4",
    "@types/json-schema": "7.0.15",
    "@types/node": "24.7.2",
    "ajv": "6.12.6",
    "better-typescript-lib": "2.12.0",
    "case-anything": "3.1.2",
    "jiti": "2.6.1",
    "json-diff-ts": "4.8.2",
    "package-directory": "8.1.0",
    "tsx": "4.20.6",
    "type-fest": "5.0.1"
  }
}
